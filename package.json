      "cookie-parser": "^1.4.7",
      "cors": "^2.8.5",
      "cross-env": "^10.0.0",
      "dotenv-flow": "^4.1.0",
      "express": "^4.16.2",
      "express-async-errors": "^3.1.1",
      "helmet": "^8.1.0",
      "jsonwebtoken": "^9.0.3",
      "mongoose": "^8.18.2",
      "rate-limiter-flexible": "^7.3.2",
      "socket.io": "^4.8.1",
      "source-map-support": "^0.5.21",
      "telegraf": "^4.16.3",
