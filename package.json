{
   "name": "template-backend",
   "version": "1.0.0",
   "main": "dist/index.js",
   "scripts": {
      "build": "npx tsc",
      "dev": "cross-env NODE_ENV=development nodemon --legacy-watch src/index.ts",
      "start": "cross-env NODE_ENV=production node dist/index.js",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "format:check": "prettier . --check",
      "format:fix": "prettier . --write",
      "prepare": "husky",
      "migrate:dev": "cross-env MIGRATE_MODE=development node script/migration.js",
      "migrate:prod": "cross-env MIGRATE_MODE=production node script/migration.js"
   },
   "lint-staged": {
      "*.ts": [
         "npm run lint:fix",
         "npm run format:fix"
      ]
   },
