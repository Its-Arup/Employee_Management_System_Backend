      "@types/node": "^24.10.9",
      "@types/source-map-support": "^0.5.10",
      "eslint": "^9.36.0",
      "eslint-config-prettier": "^10.1.8",
      "husky": "^9.1.7",
      "lint-staged": "^16.2.0",
      "nodemon": "^3.1.10",
      "prettier": "^3.6.2",
      "ts-node": "^10.9.2",
